<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<listeners>
		<listener class-name="utils.ListenerClass"></listener>
	</listeners>
	<parameter name="browser" value="Chrome"></parameter>
	<parameter name="url" value="https://thinking-tester-contact-list.herokuapp.com/"></parameter>
	<test thread-count="5" name="SignupTest">
		<classes>
			<class name="test.TC001_SignupTest" />
		</classes>
	</test> <!-- Test -->
	<test thread-count="5" name="LoginTest">
		<classes>
			<class name="test.TC002_LoginTest" />
		</classes>
	</test> <!-- Test -->
	<test thread-count="5" name="AddNewContactTest">
		<classes>
			<class name="test.TC003_AddNewContact" />
		</classes>
	</test> <!-- Test -->
	<test thread-count="5" name="EditContactTest">
		<classes>
			<class name="test.TC004_EditContact" />
		</classes>
	</test> <!-- Test -->
	<test thread-count="5" name="DeleteContactTest">
		<classes>
			<class name="test.TC005_DeleteContactAndLogout" />
		</classes>
	</test> <!--Test -->
</suite> <!--Suite -->
